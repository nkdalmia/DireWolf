
<div class="page-header">
  <h1>Profile</h1>
</div>

<table class="table table-bordered">
  <tbody>
    <tr>
      <td>Name</td>
      <td><%= @user.name %></td>
    </tr>
    <tr>
      <td>Phone</td>
      <td><%= @user.phone %></td>
    </tr>
    <tr>
      <td>Email</td>
      <td><%= @user.email %></td>
    </tr>
    <tr>
      <td>Skills</td>
      <td><%= @user.skill_list %></td>
    </tr>
  </tbody>
</table>

<div class="page-header">
  <h1>Resume</h1>
</div>

<%= (@user.resume == nil ? "" : @user.resume).gsub(/\n/, '<br>').html_safe %>

<br><br><%= link_to 'Edit', edit_user_path(@user), :class => "btn btn-default"%><br><br>





